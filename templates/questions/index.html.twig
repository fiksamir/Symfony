{% extends "base.html.twig" %}

{% block title %}
    {{ parent() }}
    Question page
{% endblock %}


{% block body %}
    <h1>{{ question }}</h1>

    <h2>Answers({{ answers|length }})</h2>
    <ul>
        {% for answer in answers %}
            <li>{{ answer }}</li>
        {% endfor %}
    </ul>
{% endblock %}